<div class="container my-5">
    <div class="post-title">
        <strong><h2>管理者ログイン</h2></strong>
          <br><br>
    <%= form_with model: @admin, url: admin_session_path do |f| %> <!-- URLの変更 -->
        <div class="row">
            <div class="field">
              <%= f.label :メールアドレス %><br />
              <%= f.email_field :email, autofocus: true, autocomplete: "email" %>
            </div>
        </div>
        <div class="row">
            <div class="field">
              <%= f.label :パスワード %><br />
              <%= f.password_field :password, autocomplete: "current-password" %>
            </div>
        </div>
        <div class="row">
            <div class="submit">
              <%= f.submit "ログイン" %>
            </div>
        </div>
    <% end %>
    </div>
</div>

<%= render "admin/shared/links" %> <!-- ここは記入しておく？ -->